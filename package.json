{
  "name": "drinaluza",
  "version": "1.0.69",
  "main": "expo-router/entry",
  "description": "Drinaluza, seafood business manager (mobile and web client)",
  "author": "Ahmed Derbala <derbala.ahmed531992@gmail.com>",
  "private": true,
  "engines": {
    "node": "24"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmed-derbala/drinaluza-expo"
  },
  "keywords": [
    "drinaluza"
  ],
  "scripts": {
    "ftr:local": "npm run i && npm run start:local",
    "cpapk2gdrive": "cp ./builds/$npm_package_name-$npm_package_version.apk /run/user/1000/gvfs/google-drive:host=gmail.com,user=drinaluza/0APGN7fnb5BgmUk9PVA/1euN1ogdssvbiq4wJdxYQBYqMXWbwIpBm",
    "env:prod": "cp envs/.env.prod .env",
    "env:local": "cp envs/.env.local .env",
    "ver": "npm version patch --no-git-tag-version",
    "build:apk": "clear && npm run env:prod && export $(grep -v '^#' .env | xargs) && NODE_ENV=production eas build -p android --profile production --local --output=./builds/$npm_package_name-$npm_package_version.apk && npm run cpapk2gdrive && npm run ver",
    "build:web": "npm run clean && NODE_ENV=production npx expo export -p web",
    "start": "expo start",
    "start:local": "rm -rf .expo && npm run env:local && expo start -c",
    "dev": "npm run env:local && expo start",
    "dev:c": "rm -rf .expo && npm run env:local && expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "i": "npm i",
    "i:prod": "NODE_ENV=production npm ci --omit=dev",
    "i:check": "echo y | npx expo install --check",
    "fix": "npm run i:check && npx expo-doctor",
    "rmallexceptpackagelock": "rm -rf builds/* logs/* dist android ios .expo node_modules",
    "clean:only": "npm run rmallexceptpackagelock && rm -rf package-lock.json",
    "clean:prod": "npm run rmallexceptpackagelock && npm run i:prod",
    "clean": "npm run clean:only && npm run i",
    "code": "antigravity . && npm run start:local",
    "update": "npm i -g eas-cli && npm run clean:only && npm run check-updates && npm run clean && npm run ver",
    "check-updates": "npm i -D npm-check-updates && ncu -u",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "push": "npm run format && git add . && git commit -m",
    "postpush": "git push && npm run ver",
    "pull": "git pull origin main && npm run i && npm run start:local"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "axios": "^1.13.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.4",
    "expo": "~54.0.33",
    "expo-build-properties": "~1.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.13",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.16",
    "lodash": "^4.17.23",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.2"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@types/lodash": "^4.17.23",
    "@types/react": "~19.1.10",
    "@types/react-native-dotenv": "^0.2.2",
    "eas-local-cache": "^1.0.3",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.17",
    "npm-check-updates": "^19.3.2",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "jest": {
    "preset": "jest-expo"
  }
}
